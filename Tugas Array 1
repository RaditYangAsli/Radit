package TugasArray;

public class Number1 {
    public static void main(String[] args) {
        int bilangan [] = new int [5];
        bilangan [0] = 1;
        bilangan [1] = 2;
        bilangan [2] = 3;
        bilangan [3] = 4;
        bilangan [4] = 5;
        System.out.println(bilangan [6]);
        //Eror karena jumlah Array nya hanya 5

    }
}
