package array;

public class ArrayMulti {

    public static void main(String[] args) {
        int nis[][] = {{210651}, {210651}, {210652}};
        String nama[][] = {{"Ade"}, {"Galih "}, {"Baihaqi"}};
        for (int i = 0; i < 1; i++) {
            for (int j = 0; j < 1; j++) {
                System.out.println(nama[1][j] + ":" + nis[1][j]);
            }

        }
    }
}
